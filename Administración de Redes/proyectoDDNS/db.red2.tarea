;
; BIND data file for local loopback interface
;
$TTL	604800
@	IN	SOA	ServerDDNS.red2.tarea root.red2.tarea (
			      2		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	localhost.
@	IN	A	127.0.0.1
@	IN	AAAA	::1

localhost	IN	A	127.0.0.1
ServerDDNS	IN	A	10.0.0.1
